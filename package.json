{
  "name": "rick-and-morty-front-end",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "depCheck": "depcheck",
    "format:write": "prettier --write \"**/*.{ts,tsx,md,json, html, css}\" --cache",
    "clean:docs": "rimraf temp-docs README.html README.md",
    "clean": "rimraf dist node_modules .pnpm-store .pnpm-lock.yaml coverage .nyc_output package-lock.json",
    "generateTempDocs": "typedoc **/**.ts* --plugin typedoc-plugin-markdown --theme markdown --out temp-docs",
    "concatTempDocs": "concat-md --toc --decrease-title-levels --dir-name-as-title temp-docs > README.md",
    "makeHtmlDocs": "showdown makehtml -i README.md -o README.html",
    "generateDocs": "pnpm run clean:docs && pnpm run generateTempDocs && pnpm run concatTempDocs && pnpm run makeHtmlDocs"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "graphql": "^16.8.1",
    "react-dom": "^18.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "showdown": "^2.1.0",
    "typedoc": "^0.25.7",
    "concat-md": "^0.5.1",
    "depcheck": "^1.4.7",
    "prettier": "^3.2.5",
    "typedoc-plugin-markdown": "^3.17.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.13"
  }
}
